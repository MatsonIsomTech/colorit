{
    "name": "fruitstudios/colorit",
    "description": "A slick color picker fieldtype plugin for the Craft CMS 3 control panel.",
    "type": "craft-plugin",
    "version": "1.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "palette",
        "colour",
        "colours",
        "color",
        "colors",
        "swatch"
    ],
    "support": {
        "docs": "https://github.com/fruitstudios/craft-colorit/blob/master/README.md",
        "issues": "https://github.com/fruitstudios/craft-colorit/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Fruit Studios",
            "homepage": "https://fruitstudios.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.2"
    },
    "autoload": {
        "psr-4": {
          "fruitstudios\\colorit\\": "src/"
        }
    },
    "extra": {
        "name": "Colorit",
        "handle": "colorit",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/fruitstudios/craft-colorit/master/CHANGELOG.md",
        "class": "fruitstudios\\colorit\\Colorit"
    }
}
