{
    "name": "fruitstudios/palette",
    "description": "Add a palette fieldtype to Craft CMS and configure your colours per field or globally.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "palette",
        "colour",
        "colours",
        "color",
        "colors"
    ],
    "support": {
        "docs": "https://github.com/fruitstudios/craft-palette/blob/master/README.md",
        "issues": "https://github.com/fruitstudios/craft-palette/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Fruit Studios",
            "homepage": "https://fruitstudios.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.2"
    },
    "autoload": {
        "psr-4": {
          "fruitstudios\\palette\\": "src/"
        }
    },
    "extra": {
        "name": "Palette",
        "handle": "palette",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/fruitstudios/craft-palette/master/CHANGELOG.md",
        "class": "fruitstudios\\palette\\Palette"
    }
}
